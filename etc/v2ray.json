{
  "log": {
    "access": "/dev/stdout",
    "error": "/dev/stderr",
    "loglevel": "warning"
  },
  "inbound": {
    "port": {{V2RAY_HTTP_PORT}},
    "protocol": "vmess",
    "settings": {
      "udp": true,
      "clients": [
        {
          "id": "{{V2RAY_UUID}}",
          "level": 1,
          "alterId": 32
        }
      ],
      "default": {
        "level": 1,
        "alterId": 32
      }
    },
    "streamSettings": {
      "network": "tcp",
      "security": "auto",
      "tcpSettings": {
        "connectionReuse": true,
        "header": {
          "type": "http",
          "request": {
            "version": "1.1",
            "method": "GET",
            "path": [
              "/"
            ],
            "headers": {
              "Host": [
                "www.apple.com",
                "www.bing.com",
                "www.amazon.com",
                "windowsupdate.microsoft.com",
                "www.cloudflare.com"
              ],
              "User-Agent": [
                "Mozilla/5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/55.0.2883.75 Safari/537.36",
                "Mozilla/5.0 (iPhone; CPU iPhone OS 10_0_2 like Mac OS X) AppleWebKit/601.1 (KHTML, like Gecko) CriOS/53.0.2785.109 Mobile/14A456 Safari/601.1.46",
                "Mozilla/5.0 (iPad; CPU OS 10_3_2 like Mac OS X) AppleWebKit/603.2.4 (KHTML, like Gecko) Version/10.0 Mobile/14F89 Safari/602.1",
                "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_11_6) AppleWebKit/537.36 (KHTML, like Gecko) electron/1.0.0 Chrome/53.0.2785.113 Electron/1.4.3 Safari/537.36"
              ],
              "Accept-Encoding": [
                "gzip, deflate"
              ],
              "Connection": [
                "keep-alive"
              ],
              "Pragma": "no-cache"
            }
          },
          "response": {
            "version": "1.1",
            "status": "200",
            "reason": "OK",
            "headers": {
              "Content-Type": [
                "application/octet-stream",
                "video/mpeg",
                "application/x-msdownload",
                "text/html",
                "application/x-shockwave-flash"
              ],
              "Transfer-Encoding": [
                "chunked"
              ],
              "Connection": [
                "keep-alive"
              ],
              "Pragma": "no-cache"
            }
          }
        }
      }
    }
  },
  "inboundDetour": [
    {
      "port": {{V2RAY_TLS_PORT}},
      "protocol": "vmess",
      "settings": {
        "udp": true,
        "clients": [
          {
            "id": "{{V2RAY_UUID}}",
            "level": 1,
            "alterId": 32
          }
        ],
        "streamSettings": {
          "network": "tcp",
          "security": "tls",
          "tcpSettings": {
            "connectionReuse": true,
            "header": {
              "type": "none"
            }
          },
          "tlsSettings": {
            "serverName": "{{V2RAY_DOMAINS}}",
            "certificates": [
              {
                "certificateFile": "/srv/certs/{{V2RAY_DOMAINS}}.crt",
                "keyFile": "/srv/certs/{{V2RAY_DOMAINS}}.key"
              }
            ]
          }
        }
      }
    },
    {
      "protocol": "vmess",
      "port": {{V2RAY_WS_PORT}},
      "settings": {
        "udp": true,
        "clients": [
          {
            "id": "{{V2RAY_UUID}}",
            "alterId": 32,
            "level": 1
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "serverName": "{{V2RAY_DOMAINS}}",
          "allowInsecure": true,
          "certificates": [
            {
              "certificateFile": "/srv/certs/{{V2RAY_DOMAINS}}.crt",
              "keyFile": "/srv/certs/{{V2RAY_DOMAINS}}.key"
            }
          ]
        },
        "wsSettings": {
          "connectionReuse": true,
          "path": "/"
        }
      }
    }
  ],
  "outbound": {
    "protocol": "freedom",
    "settings": {}
  },
  "outboundDetour": [
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blocked"
    }
  ],
  "routing": {
    "strategy": "rules",
    "settings": {
      "rules": [
        {
          "type": "field",
          "ip": [
            "0.0.0.0/8",
            "10.0.0.0/8",
            "100.64.0.0/10",
            "127.0.0.0/8",
            "169.254.0.0/16",
            "172.16.0.0/12",
            "192.0.0.0/24",
            "192.0.2.0/24",
            "192.168.0.0/16",
            "198.18.0.0/15",
            "198.51.100.0/24",
            "203.0.113.0/24",
            "::1/128",
            "fc00::/7",
            "fe80::/10"
          ],
          "outboundTag": "blocked"
        }
      ]
    }
  }
}
